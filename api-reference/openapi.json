                    },
                    "dall_e": {
                        "$ref": "#/components/schemas/AIONPredictionBase"
                    },
                    "stable_diffusion": {
                        "$ref": "#/components/schemas/AIONPredictionBase"
                    },
                    "this_person_does_not_exist": {
                        "$ref": "#/components/schemas/AIONPredictionBase"
                    },
                    "adobe_firefly": {
                        "$ref": "#/components/schemas/AIONPredictionBase"
                    },
                    "flux": {
                        "$ref": "#/components/schemas/AIONPredictionBase"
                    },
                    "four_o": {
                        "$ref": "#/components/schemas/AIONPredictionBase"
                    }
                },
                "type": "object",
                "required": [
                    "midjourney",
                    "dall_e",
                    "stable_diffusion",
                    "this_person_does_not_exist",
                    "adobe_firefly",
                    "flux",
                    "four_o"
                ],
                "title": "AIONGeneratorScheme"
            },
            "AIONPredictionBase": {
                "properties": {
                    "is_detected": {
                        "type": "boolean",
                        "title": "Is Detected"
                    },
                    "confidence": {
                        "type": "number",
                        "title": "Confidence"
                    }
                },
                "type": "object",
                "required": [
                    "is_detected",
                    "confidence"
                ],
                "title": "AIONPredictionBase"
            },
            "AIONPredictionType2": {
                "properties": {
                    "is_detected": {
                        "type": "boolean",
                        "title": "Is Detected"
                    }
                },
                "type": "object",
