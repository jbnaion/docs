                        "default": "1.0.0"
                    },
                    "is_detected": {
                        "type": "boolean",
                        "title": "Is Detected"
                    },
                    "labels": {
                        "items": {},
                        "type": "array",
                        "title": "Labels"
                    }
                },
                "type": "object",
                "required": [
                    "is_detected",
                    "labels"
                ],
                "title": "NSFWReportSchemeT4"
