            "QualityReportScheme": {
                "properties": {
                    "is_detected": {
                        "type": "boolean",
                        "title": "Is Detected"
                    }
                },
                "type": "object",
                "required": [
                    "is_detected"
                ],
                "title": "QualityReportScheme"
            },
            "RoiReportScheme": {
                "properties": {
                    "bbox": {
                        "$ref": "#/components/schemas/BboxScheme"
