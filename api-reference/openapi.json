                    },
                    "ai": {
                        "$ref": "#/components/schemas/AIONPredictionType2"
                    },
                    "human": {
                        "$ref": "#/components/schemas/AIONPredictionType2"
                    }
                },
                "type": "object",
                "required": [
                    "verdict",
                    "ai",
                    "human"
                ],
                "title": "AIONReportSchemeT2"
            },
            "AudioReportResponse": {
                "properties": {
                    "report": {
                        "$ref": "#/components/schemas/ShareReport"
